{
  "$schema": "http://json-schema.org/schema#",
  "$id": "function_scope_schema.json",
  "type": "object",
  "additionalProperties": false,
  "description": "function scope",
  "required": ["bucket", "scope"],
  "properties": {
    "bucket": {
      "type": "string",
      "description": "bucket to which function belongs",
      "minLength": 1,
      "default": "*"
    },
    "scope": {
      "type": "string",
      "description": "scope to which function belongs",
      "minLength": 1,
      "default": "*"
    }
  }
}
